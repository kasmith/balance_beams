doSPRT = function(ps, thresh = .75) {
cnts = rep(1,length(ps))
while(max(cnts)/sum(cnts) < thresh) {
cnts = cnts + rmultinom(1,1,ps)
}
n = sum(cnts) - 3
maxidx = which(cnts == max(cnts))
return(c(maxidx,n))
}
doSPRT(ps,.75)
